<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>info.stovell.KeepDriveSpinning.MEDIA3</string>
    <key>ProgramArguments</key>
    <array>
      <string>/bin/bash</string>
      <string>-c</string>
      <string>if [[ -w '/Volumes/MEDIA3'/.keepThisDriveSpinning ]]; then date '+%F %T %Z' &gt; '/Volumes/MEDIA3'/.keepThisDriveSpinning; fi</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>StartInterval</key>
    <integer>60</integer>
  </dict>
</plist>
